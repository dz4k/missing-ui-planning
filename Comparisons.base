      - property: Framework independent
        direction: DESC
      - property: Framework-free
        direction: DESC
      - property: Headless
        direction: DESC
      - property: Has styles
        direction: DESC
      - property: Styleable
        direction: DESC
      - property: Buildless
        direction: DESC
      - property: Has styles
        direction: DESC
    columnSize:
      note.Component count (approx.): 68
      note.Framework independent: 143
      note.License: 164
